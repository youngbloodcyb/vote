<div id="<%= dom_id user_vote %>">
  <% if action_name != "show" %>
    <%= link_to "Show this user vote", user_vote, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this user vote', edit_user_vote_path(user_vote), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
